#CONEXION CON LA BASE DE DATOS
spring.jpa.database=POSTGRESQL
#spring.datasource.platform=postgres
spring.sql.init.platform=postgres
spring.datasource.url=jdbc:postgresql://localhost:5432/DB_SistemaVacKruger
spring.datasource.username=postgres
spring.datasource.password=obelisco
spring.jpa.show-sql=true

#CREAR TABLAS DESDE LOS MODELOS - COMENTAR DESPUES DE LA PRIMERA EJECUCION O SE RESETEARAN LAS TABLAS DE LA DB
spring.jpa.generate-ddl=true
spring.jpa.hibernate.ddl-auto=create-drop
spring.jpa.properties.hibernate.jdbc.lob.non_contextual_creation=true
